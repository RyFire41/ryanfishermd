    "globals": "^16.4.0",
    "postcss": "^8.5.6",
    "puppeteer": "^24.26.0",
    "rollup-plugin-visualizer": "^5.9.0",
    "sharp": "^0.32.4",
    "tailwindcss": "^4.1.14",
    "vite": "^7.1.7"
