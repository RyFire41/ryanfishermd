  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "postbuild": "node scripts/generate-sitemap.js",
    "prebuild": "node scripts/convert-images.js",
    "convert-images": "node scripts/convert-images.js",
    "analyze": "npm run build && npx rollup-plugin-visualizer --help",
